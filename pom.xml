	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.3.2</version>
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<compilerArgument>-Xlint:all</compilerArgument>
					<showWarnings>true</showWarnings>
					<showDeprecation>true</showDeprecation>
				</configuration>
			</plugin>

			<plugin>
				<groupId>com.felees</groupId>
				<artifactId>hbnpojogen-maven-plugin</artifactId>
				<version>1.4.32</version>
				<inherited>false</inherited>
				<configuration>
					<!-- Default is: hbnpojogen.config.xml -->
					<!-- <configFile>relative/path/to/synchronizer.config.xml</configFile> -->

					<!-- The child artifact that will contain the model. -->
					<modelArtifactId>demo-model</modelArtifactId>

					<!-- Override the database ip config file setting. -->
					<!-- <ip>127.0.0.1</ip> -->
				</configuration>
				<executions>
					<execution>
						<goals>
							<goal>generate</goal>
						</goals>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
